{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"baseUrl": "./",
		"paths": {
			"@assets/*": ["src/assets/*"],
			"@common/*": ["./src/common/*"],
			"@components/*": ["./src/components/*"],
			"@contexts/*": ["./src/contexts/*"],
			"@routes/*": ["./src/routes/*"],
			"@services/*": ["./src/services/*"],
			"@screens/*": ["./src/screens/*"],
			"@utils/*": ["./src/utils/*"]
		}
	}
}
