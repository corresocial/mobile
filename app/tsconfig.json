{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"baseUrl": "./",
		"paths": {
			"@contexts/*": [
				"./src/contexts/*"
			],
			"@routes/*": [
				"./src/routes/*"
			],
			"@services/*": [
				"./src/services/*"
			],
			"@presentation/*": [
				"./src/presentation/*"
			],
			"@domain/*": [
				"./src/domain/*"
			],
			"@infrastructure/*": [
				"./src/infrastructure/*"
			],
			"@data/*": [
				"./src/data/*"
			],
			"@utils/*": [
				"./src/utils/*"
			],
		}
	}
}