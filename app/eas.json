{
	"cli": {
		"version": ">= 3.6.0",
		"appVersionSource": "local",
		"promptToConfigurePushNotifications": false
	},
	"build": {
		"development-build": {
			"developmentClient": true,
			"distribution": "internal",
			"ios": {
				"simulator": true,
				"resourceClass": "medium"
			},
			"android": {
				"buildType": "apk"
			},
			"env": {},
			"channel": "development-build"
		},
		"development": {
			"developmentClient": false,
			"distribution": "internal",
			"ios": {
				"simulator": true,
				"resourceClass": "medium"
			},
			"android": {
				"buildType": "apk",
				"resourceClass": "medium"
			},
			"env": {
				"EAS_BUILD_ENABLE_COCOAPODS_CACHE": "1"
			},
			"channel": "development"
		},
		"production": {
			"ios": {
				"resourceClass": "medium",
				"applicationArchivePath": "ios/build"
			},
			"android": {
				"resourceClass": "large",
				"applicationArchivePath": "android/app/build/outputs/**/*.{apk,aab}"
			},
			"env": {
				"EAS_BUILD_ENABLE_COCOAPODS_CACHE": "1"
			},
			"channel": "v0.11.0"
		}
	},
	"submit": {
		"production": {}
	}
}
